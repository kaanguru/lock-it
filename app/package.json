{
  "name": "lock-it",
  "version": "1.3.0",
  "private": true,
  "description": " Offline hardware and software asset management tool for IT professionals.",
  "keywords": [
    "dexie",
    "superforms",
    "sveltekit"
  ],
  "license": "GPL-3.0-only",
  "author": "CEM KAAN KOSALI",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "preview": "vite preview",
    "test": "npm run test:pl && npm run test:unit",
    "test:pl": "playwright test",
    "test:unit": "vitest",
    "ver": "npm version patch -m 'patched'"
  },
  "dependencies": {
    "@floating-ui/dom": "1.6.5",
    "@vincjo/datatables": "^1.14.10",
    "dexie": "^4.0.8",
    "iconify-icon": "^2.1.0"
  },
  "devDependencies": {
    "@anatine/zod-mock": "^3.13.4",
    "@faker-js/faker": "^8.4.1",
    "@playwright/test": "^1.47.0",
    "@skeletonlabs/skeleton": "^2.10.2",
    "@skeletonlabs/tw-plugin": "^0.4.0",
    "@sveltejs/adapter-static": "^3.0.4",
    "@sveltejs/kit": "^2.5.26",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "^0.5.15",
    "@types/estree": "^1.0.5",
    "@types/node": "^20.16.5",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "autoprefixer": "^10.4.20",
    "crypto-js": "^4.2.0",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.43.0",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "svelte": "^5.0.0-next.244",
    "svelte-check": "^3.8.6",
    "sveltekit-superforms": "^2.17.0",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.6.2",
    "vite": "^5.4.4",
    "vite-plugin-tailwind-purgecss": "^0.3.3",
    "vitest": "^1.6.0",
    "zod": "^3.23.8"
  }
}
